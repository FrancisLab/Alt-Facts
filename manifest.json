{
    "manifest_version": 2,
    "name": "v chill",
    "description": "make your browser v chill",
    "version": "1.0",

    "browser_action": {
        "default_title": "p chill yeah"
    },

    "permissions": [
        "activeTab"
    ],

    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "js": [ "chill.js" ],
            "run_at": "document_idle"
        }
    ]
}